<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        {# {{ encore_entry_link_tags('app') }} #}
    {% endblock %}

    {% block javascripts %}
        {# {{ encore_entry_script_tags('app') }} #}
    {% endblock %}
</head>
<body>
<header>
    <nav>
        <ul>
            <li><a href="/">Accueil</a></li>
            <li><a href="{{ path('contact') }}">Contact</a></li>
            <li><a href="{{ path('contactCity', {city: 'montpellier'}) }}">Contacter Montpellier</a></li>
            <li><a href="{{ path('contact', {name: "alex"}) }}">Contacter de la part d'Alex</a></li>
            <li><a href="{{ path('contactCity', {city: 'montpellier', name: "alex"}) }}">Contacter Montpellier de la
                    part d'Alex</a></li>
        </ul>
    </nav>
</header>
{% block body %}{% endblock %}
<footer>

</footer>
</body>
</html>
